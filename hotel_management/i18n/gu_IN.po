#. module: hotel_management
#: model:mail.template,body_html:hotel_management.room_reservation_confirmed_email_template
msgid ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.rooms_ids\" t-as=\"guests\">\n"
"                    Dear <t t-out=\"object.customer_ids[0].name\"/>,\n"
"                    <br/><br/>\n"
"                    Your reservation for <t t-out=\"guests.room_sequence\"/> is confirmed.\n"
"                    <br/><br/>\n"
"                    Regards,\n"
"                    <br/>\n"
"                    The Taj\n"
"                    </t>\n"
"                </t>\n"
"            </p>\n"
"        "
msgstr ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.rooms_ids\" t-as=\"guests\">\n"
"                    પ્રિય <t t-out=\"object.customer_ids[0].name\"/>,\n"
"                    <br/><br/>\n"
"                    <t t-out=\"guests.room_sequence\"/> માટે તમારું રિઝર્વેશન કન્ફર્મ થયું છે.\n"
"                    <br/><br/>\n"
"                    સાદર,\n"
"                    <br/>\n"
"                    તાજ\n"
"                    </t>\n"
"                </t>\n"
"            </p>"

#. module: hotel_management
#: model:mail.template,body_html:hotel_management.restaurant_guests_reservation_confirmed_email_template
msgid ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.table_booking_list_ids\" t-as=\"tables\">\n"
"                    Dear <t t-out=\"object.guest_ids[0].name\"/>,\n"
"                    <br/><br/>\n"
"                    Your reservation for <t t-out=\"tables.table_no_seq\"/> on <t t-out=\"object.reservation_date\"/> is confirmed.\n"
"                    <br/><br/>\n"
"                    Regards,\n"
"                    <br/>\n"
"                    The Taj\n"
"                    </t>\n"
"                </t>\n"
"            </p>\n"
"        "
msgstr ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.table_booking_list_ids\" t-as=\"tables\">\n"
"                    પ્રિય <t t-out=\"object.guest_ids[0].name\"/>,\n"
"                    <br/><br/>\n"
"                    <t t-out=\"object.reservation_date\"/> ના રોજ <t t-out=\"tables.table_no_seq\"/> માટે તમારું રિઝર્વેશન કન્ફર્મ થયું છે.\n"
"                    <br/><br/>\n"
"                    સાદર,\n"
"                    <br/>\n"
"                    તાજ\n"
"                    </t>\n"
"                </t>\n"
"            </p>"

