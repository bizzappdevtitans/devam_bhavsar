#. module: hotel_management
#: model:mail.template,body_html:hotel_management.room_reservation_confirmed_email_template
msgid ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.rooms_ids\" t-as=\"guests\">\n"
"                    Dear <t t-out=\"object.customer_ids[0].name\"/>,\n"
"                    <br/><br/>\n"
"                    Your reservation for <t t-out=\"guests.room_sequence\"/> is confirmed.\n"
"                    <br/><br/>\n"
"                    Regards,\n"
"                    <br/>\n"
"                    The Taj\n"
"                    </t>\n"
"                </t>\n"
"            </p>\n"
"        "
msgstr ""

#. module: hotel_management
#: model:mail.template,body_html:hotel_management.restaurant_guests_reservation_confirmed_email_template
msgid ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.table_booking_list_ids\" t-as=\"tables\">\n"
"                    Dear <t t-out=\"object.guest_ids[0].name\"/>,\n"
"                    <br/><br/>\n"
"                    Your reservation for <t t-out=\"tables.table_no_seq\"/> on <t t-out=\"object.reservation_date\"/> is confirmed.\n"
"                    <br/><br/>\n"
"                    Regards,\n"
"                    <br/>\n"
"                    The Taj\n"
"                    </t>\n"
"                </t>\n"
"            </p>\n"
"        "
msgstr ""

#. module: hotel_management
#: model:mail.template,body_html:hotel_management.restaurant_direct_reservation_confirmed_email_template
msgid ""
"<p>\n"
"                <t>\n"
"                    <t t-foreach=\"object.table_booking_list_ids\" t-as=\"tables\">\n"
"                    Dear <t t-out=\"object.guest_name\"/>,\n"
"                    <br/><br/>\n"
"                    Your reservation for <t t-out=\"tables.table_no_seq\"/> on <t t-out=\"object.reservation_date\"/> is confirmed.\n"
"                    <br/><br/>\n"
"                    Regards,\n"
"                    <br/>\n"
"                    The Taj\n"
"                    </t>\n"
"                </t>\n"
"            </p>\n"
"        "
msgstr ""

#. module: hotel_management
#: model:mail.template,body_html:hotel_management.room_reservation_reminder_email_template
msgid ""
"<p>\n"
"                Dear <t t-out=\"object.customer_ids[0].name\"/>,\n"
"                <br/><br/>\n"
"                We are excited for your upcoming stay at The Taj.\n"
"                <br/>\n"
"                As a reminder, check-in date is on <t t-out=\"object.check_in_date\"/> and check-out date is <t t-out=\"object.check_out_date\"/> with your stay totaling at <t t-out=\"object.total_days\"/> days.\n"
"                <br/>\n"
"                Thank you and we look forward to seeing you soon.\n"
"                <br/><br/>\n"
"                Regards,\n"
"                <br/>\n"
"                The Taj\n"
"            </p>\n"
"        "
msgstr ""

